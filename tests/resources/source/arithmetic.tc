int test_num;
int correct;
void atest(int exp, int act) {
    if (exp == act) {
        correct = correct + 1;
    } else {
        print_s((char*)"TEST "); print_i(test_num); print_s((char*)" FAIL "); print_i(exp); print_s((char*)" != "); print_i(act); print_c('\n');
    }
    test_num = test_num + 1;
}

void main() {
    int x; int y; int z;
    test_num = 0;
    atest(1+1, 2);
    atest(34*4, 136);
    atest(--500, 500);
    atest(667 + 4 * 4 * 33 / 6 % 7, 671);
    x = -34;
    y = -4;
    atest(x * y, 136);
    atest(x * 5 + y * 5 - 44, -234);
    atest(13 / 3, 4);
    atest(77 % 44, 33);
    atest(-13 / 3, -4);
    atest((-13 / 3) * 3 + (-13 % 3), -13);
    atest(55 * 34 + -x % 4, 1872);
    atest(1537983633 * 2, -1219000030);

    if (correct == test_num) {
        print_s((char*)"ALL PASS");
    }
}
